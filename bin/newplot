#!/bin/bash

chrome='/Applications/Google Chrome.app/Contents/MacOS/Google Chrome'
tmp=/tmp/newplot.$RANDOM.html

if [ -z "$2" ]
then
    file="-"
else
    file="$2"
fi

data=$(cat $file)
m4 -P -DOPTIONS=null -DDATA="$data" $1 > $tmp 

"$chrome" --new-window --incognito --app=file://$tmp & >/dev/null &2>1

